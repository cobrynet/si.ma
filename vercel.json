{{

  "cleanUrls": true,  "version": 2,

  "trailingSlash": false  "public": true,

}  "cleanUrls": true,

  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "www.assistenzalavanderie.it" }
      ],
      "destination": "https://assistenzalavanderie.it/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.css",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*)\\.js",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/contenuti/(.*)\\.(jpg|jpeg|png|webp|svg|gif|ico)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/contenuti/(.*)\\.(webm|mp4)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
